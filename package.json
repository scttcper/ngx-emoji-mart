{
  "name": "@ctrl/ngx-emoji-mart",
  "version": "0.0.0-placeholder",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "del dist && ts-node -O '{ \"module\": \"commonjs\" }' build.ts",
    "build-data": "ts-node --project=scripts/tsconfig.json scripts/build-data.ts",
    "test": "ng test --watch=false --browsers=ChromeCI",
    "test:watch": "ng test --browsers=ChromeCI",
    "test:ci": "ng test --watch=false --code-coverage --no-progress --browsers=ChromeCI",
    "link": "cd dist && npm link",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "ghpages": "ng build --prod --no-progress",
    "semantic-release": "cd dist && semantic-release"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "9.1.2",
    "@angular/compiler": "9.1.2",
    "@angular/core": "9.1.2",
    "@angular/forms": "9.1.2",
    "@angular/platform-browser": "9.1.2",
    "@angular/platform-browser-dynamic": "9.1.2",
    "@ctrl/ngx-github-buttons": "4.0.1",
    "bootstrap": "4.4.1",
    "core-js": "3.6.5",
    "rxjs": "6.5.5",
    "tslib": "1.11.1",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.901.4",
    "@angular/cli": "9.1.4",
    "@angular/compiler-cli": "9.1.2",
    "@angular/language-service": "9.1.2",
    "@types/inflection": "1.5.28",
    "@types/jasmine": "3.5.10",
    "@types/node": "13.11.1",
    "@types/stringify-object": "3.2.0",
    "codelyzer": "5.2.2",
    "del-cli": "3.0.0",
    "emoji-datasource": "5.0.1",
    "emojilib": "2.4.0",
    "inflection": "1.12.0",
    "jasmine-core": "3.5.0",
    "karma": "5.0.2",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "2.1.1",
    "karma-jasmine": "3.1.1",
    "karma-jasmine-html-reporter": "1.5.3",
    "karma-mocha-reporter": "2.2.5",
    "ng-packagr": "9.1.1",
    "semantic-release": "17.0.6",
    "stringify-object": "3.3.0",
