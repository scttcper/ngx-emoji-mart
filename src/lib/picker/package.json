{
  "$schema": "../../../node_modules/ng-packagr/package.schema.json",
  "name": "@ctrl/ngx-emoji-mart",
  "version": "0.0.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "Customizable Slack-like emoji picker for Angular",
  "peerDependencies": {
    "@angular/core": ">=10.0.0-0",
    "@angular/common": ">=10.0.0-0"
  },
  "homepage": "https://ngx-emoji-mart.vercel.app",
  "repository": "scttcper/ngx-emoji-mart",
  "license": "MIT",
  "sideEffects": [ "*.css" ],
  "ngPackage": {
    "lib": {
      "entryFile": "public_api.ts",
      "umdModuleIds": {
        "@ctrl/ngx-emoji-mart/ngx-emoji": "@ctrl/ngx-emoji-mart/ngx-emoji"
      }
    },
    "dest": "../../../dist"
  },
  "keywords": [
    "angular",
    "emoji",
    "picker"
  ]
}
