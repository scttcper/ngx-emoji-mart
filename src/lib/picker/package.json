{
  "$schema": "../../../node_modules/ng-packagr/package.schema.json",
  "name": "@ctrl/ngx-emoji-mart",
  "version": "0.0.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "Customizable Slack-like emoji picker for Angular",
  "peerDependencies": {
    "@angular/core": ">=8.0.0-0",
    "@angular/common": ">=8.0.0-0"
  },
  "bugs": "https://github.com/TypeCtrl/ngx-emoji-mart/issues",
  "homepage": "https://github.com/TypeCtrl/ngx-emoji-mart",
  "repository": "TypeCtrl/ngx-emoji-mart",
  "license": "MIT",
  "sideEffects": [ "*.css" ],
  "ngPackage": {
    "lib": {
      "entryFile": "public_api.ts",
      "languageLevel": ["dom", "es2017"],
      "umdModuleIds": {
        "@ctrl/ngx-emoji-mart/ngx-emoji": "@ctrl/ngx-emoji-mart/ngx-emoji"
      }
    },
    "dest": "../../../dist/picker"
  },
  "keywords": [
    "angular",
    "emoji",
    "picker"
  ]
}
